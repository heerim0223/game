<body>
    <h2>Moving Mouse Without Mouse</h2>
	<div class="modal">
		<div class="modal_popup"> <!-- 메뉴창 -->
			<h3>Menu</h3>
			<p>------------------------------------------------------------------</p>
			<table> 
				<tr>
					<td><button class="menu-icon" id="btn_doc" style="border: 0px; background-color: rgba(0,0,0,0);">📘</button></td>
					<td><button class="menu-icon" id="btn_mon" style="border: 0px; background-color: rgba(0,0,0,0);">📟</button></td>
					<td><button class="menu-icon" id="btn_bag" style="border: 0px; background-color: rgba(0,0,0,0);">💼</button></td>
					<td><button class="menu-icon" id="btn_set" style="border: 0px; background-color: rgba(0,0,0,0);">⚙️</button></td>
				</tr>
				<tr class="menu-text">
					<td>도감</td>
					<td>포켓몬</td>
					<td>가방</td>
					<td>설정</td>
				</tr>
			</table>
		</div>
	</div>
    <canvas id="canvas"></canvas>
    <script src="script.js"></script>
</body>

